		gpios = <3 GPIO_ACTIVE_LOW>;
		output-high;
		gpio-hog;
	};

	lb4 {
		line-name = "beamforming:2g:lb4";
		gpios = <4 GPIO_ACTIVE_LOW>;
		output-high;
		gpio-hog;
	};

	lb5 {
		line-name = "beamforming:2g:lb5";
		gpios = <5 GPIO_ACTIVE_LOW>;
		output-high;
		gpio-hog;
	};

	lb6 {
		line-name = "beamforming:2g:lb6";
